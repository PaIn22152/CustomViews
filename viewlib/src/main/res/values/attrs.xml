<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="PieChartView">
    <attr format="boolean"
      name="clockwise"/><!--顺时针-->
    <attr format="color"
      name="pie_color"/><!--背景色-->
    <attr format="integer"
      name="start_angle"/><!--起始点角度-->
  </declare-styleable>


  <declare-styleable name="DialProcessBar">
    <attr format="color"
      name="dark_color"/><!--暗色-->
    <attr format="color"
      name="light_color"/><!--亮色-->
    <attr format="integer"
      name="scale_num"/><!--刻度条数量-->
    <attr format="boolean"
      name="show_pointer"/><!--是否显示指针-->

  </declare-styleable>
</resources>